#: Ooze_Mini_Campaign/scenarios/4_Pinewood_Siege.cfg:1605
#: Ooze_Mini_Campaign/scenarios/4_mp_Pinewood_Siege.cfg:2833
msgid "No! I'm too pretty to die!"
msgstr "Нет! Я слишком красива, что бы умереть!"

#. [then]
#: Ooze_Mini_Campaign/scenarios/4_Pinewood_Siege.cfg:1612
#: Ooze_Mini_Campaign/scenarios/4_mp_Pinewood_Siege.cfg:2840
msgid "Corinna! NOOOOO!!!!"
msgstr "Карина! Не-е-е-ет!"

#. [then]
#: Ooze_Mini_Campaign/scenarios/4_Pinewood_Siege.cfg:1616
#: Ooze_Mini_Campaign/scenarios/4_mp_Pinewood_Siege.cfg:2844
msgid "Wracked with grief, Sir Dryden has abandoned the fight!"
msgstr "Убитый горем, сэр Драйден покинул поле боя!"

#. [multiplayer]: id=4_mp_Pinewood_Siege, id=Stern Guard, id=Bored Guard, id=Ram Aide, id=High Priest Jal, id=Jedd, id=Corinna, id=Sergeant Ryn, id=Frist, id=Valin, id=Brock, id=Kirielle the Realist, id=Bryony the Idealist, id=Bored Guard, id=Stern Guard, id=Shady Character, id=Captain Koltz, id=Imperial Mage, id=Blackfang, id=Blackfang, id=Master Ooze, id=Master Ooze 2
